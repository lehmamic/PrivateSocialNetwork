// Place your settings in this file to overwrite default and user settings.
{
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"node_modules": true,
		"**/*.js": { "when": "$(basename).ts"},
		"**/*.css": { "when": "$(basename).scss"},
		"**/*.css.map": { "when": "$(basename).scss"}
	}
}